{
    "manifest_version": 3,
    "name": "UG-Scroller",
    "version": "1.1",
    "description": "An extension to scroll automatically on Ultimate Guitar",
    "icons": {
        "128": "icons/icon_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://tabs.ultimate-guitar.com/tab/*"
            ],
            "js": [
                "autoscroll.js"
            ]
        }
    ]
}